{
  "name": "cypress-cucumber-typescript-starter",
  "version": "1.0.0",
  "description": "A structured template for using Cypress with Cucumber and TypeScript",
  "scripts": {
    "start": "cypress open",
    "test": "cypress run --spec \"**/*.feature\"",
    "lint": "eslint ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vdsabev/cypress-cucumber-typescript-starter.git"
  },
  "keywords": [
    "cypress",
    "cucumber",
    "automation",
    "testing"
  ],
  "author": "Vladimir Sabev",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vdsabev/cypress-cucumber-typescript-starter/issues"
  },
  "homepage": "https://github.com/vdsabev/cypress-cucumber-typescript-starter#readme",
  "devDependencies": {
    "@shelex/cypress-allure-plugin": "2.39.1",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "5.57.1",
    "cypress": "^12.12.0",
    "cypress-cucumber-preprocessor": "^4.3.1",
    "cypress-mochawesome-reporter": "^3.7.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "typescript": "^4.9.5"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true,
    "stepDefinitions": "cypress/e2e",
    "nonGlobalStepBaseDir": "cypress/e2e",
    "commonPath": "common"
  }
}
